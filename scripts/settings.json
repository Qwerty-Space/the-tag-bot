{
  "settings": {
    "analysis": {
      "analyzer": {
        "ascii_fold": {
          "type": "custom",
          "tokenizer": "standard",
          "char_filter": ["split_filter"],
          "filter": ["lowercase", "asciifolding"]
        }
      },
      "char_filter": {
        "split_filter": {
          "type": "mapping",
          "mappings": [
            "_ => -"
          ]
        }
      }
    }
  },
  "mappings": {
    "dynamic": false,
    "properties": {
      "access_hash": {
        "type": "keyword",
        "index": false
      },
      "title": {
        "type": "search_as_you_type",
        "analyzer": "ascii_fold",
        "doc_values": false,
        "max_shingle_size": 3
      },
      "created": {
        "type": "long"
      },
      "emoji": {
        "type": "keyword",
        "ignore_above": 256
      },
      "ext": {
        "type": "search_as_you_type",
        "analyzer": "ascii_fold",
        "doc_values": false,
        "max_shingle_size": 3
      },
      "filename": {
        "type": "search_as_you_type",
        "analyzer": "ascii_fold",
        "doc_values": false,
        "max_shingle_size": 3
      },
      "id": {
        "type": "keyword",
        "ignore_above": 256
      },
      "is_animated": {
        "type": "boolean"
      },
      "last_used": {
        "type": "long"
      },
      "owner": {
        "type": "keyword",
        "ignore_above": 256
      },
      "pack_link": {
        "type": "keyword",
        "index": false
      },
      "pack_name": {
        "type": "search_as_you_type",
        "analyzer": "ascii_fold",
        "doc_values": false,
        "max_shingle_size": 3
      },
      "tags": {
        "type": "search_as_you_type",
        "analyzer": "ascii_fold",
        "doc_values": false,
        "max_shingle_size": 3
      },
      "type": {
        "type": "keyword",
        "ignore_above": 256
      }
    }
  }
}